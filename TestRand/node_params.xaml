<UserControl x:Class="TestRand.node_params"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background=" #112233">
        <StackPanel Name="stp" Margin="0,10,0,-10">
            <Label > Radius : </Label>
            <TextBox Name="radius"></TextBox>
            <Label > Comm Range :</Label>
            <TextBox Name="node_cr"></TextBox>
            <Label> Energy</Label>
            <TextBox Name="energy"></TextBox>
            <Button x:Name="Save" Click="Save_Click" Content="Save" Margin="10"/>
        </StackPanel>
    </Grid>
</UserControl>
